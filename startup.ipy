%load_ext autoreload

import importlib
import sys

sys.path.append("src")

YEAR = 2020
DAY = 5

def load_day(year, day):
    year_part = year % 2000
    day_part = str(day).rjust(2, "0")

    return importlib.import_module(f"year_{year}.day_{year_part}_{day_part}")


D = load_day(YEAR, DAY)

def recompile():
    importlib.reload(D)
